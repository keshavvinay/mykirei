@charset "utf-8";
/*corp-headingLv2-01*/
.corp-headingLv2-01 {
  .l-HeadingTitle__h2 {
    border-left: 6px solid $accent!important;
    .cmn-richtext {
      span {
        margin-left: 15px;
        display: block;
      }
    } 
  }
  @include bp(lg) {
    .l-HeadingTitle__h2 {
      font-size: 32px;
      margin-bottom: 28px;
      line-height: 1;
    }
  }
  @include bp(md) {
    .l-HeadingTitle__h2 {
      font-size: 28px;
      margin-bottom: 15px;
    }
  }
  @include bp(sm) {
    .l-HeadingTitle__h2 {
      font-size: 22px;
      margin-bottom: 12px;
    }
  }
  .l-HeadingTitle__titleBlock__titleBox__title {
    margin-bottom: 0;
  }   
}
.corp-headingLv2-01.l-HeadingTitle--h2  {
  border-bottom: 1px solid #d9d9d9;
}


