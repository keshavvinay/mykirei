@charset "utf-8";

/*****************************
  Content Color set (essential)
 *****************************/

/* Body */
$body__bg: #fff;

/* Brand Header */
$brandHeader__bg: #fff;
$brandHeader__text: #592404; //essential
$brandHeader__icon: #999;
$brandHeader__border: #ccc;
$brandHeader__button__bg: #592404; //essential
$brandHeader__button__icontext: #fff;

$brandHeader__linkNormal__bg: transparent;
$brandHeader__linkNormal__text: #592404; //essential
$brandHeader__linkNormal__icon: #999;
$brandHeader__linkNormal__bg--hover: #eee;
$brandHeader__linkNormal__text--hover: #592404; //essential
$brandHeader__linkNormal__icon--hover: #666;

$brandHeader__linkCurrent__bg: #eee;
$brandHeader__linkCurrent__text: #592404; //essential
$brandHeader__linkCurrent__icon: #666;
$brandHeader__linkCurrent__bg--hover: #ccc;
$brandHeader__linkCurrent__text--hover: #592404; //essential
$brandHeader__linkCurrent__icon--hover: #666;

$brandHeader__separateBorder--top: #ccc;
$brandHeader__separateBorder--bottom: #ccc;

/* Local Navigation */
$localNavigation__bg: #f1f1f1;
$localNavigation__bg--smmd: #fff;
$localNavigation__text: #592404; //essential
$localNavigation__icon: #999;
$localNavigation__border: #ccc;

$localNavigation__linkNormal__bg: transparent;
$localNavigation__linkNormal__text: #333;
$localNavigation__linkNormal__icon: #999;
$localNavigation__linkNormal__bg--hover: #ccc;
$localNavigation__linkNormal__text--hover: #333;
$localNavigation__linkNormal__icon--hover: #666;

$localNavigation__linkCurrent__bg: #ccc;
$localNavigation__linkCurrent__text: #333;
$localNavigation__linkCurrent__icon: #666;
$localNavigation__linkCurrent__bg--hover: #aaa;
$localNavigation__linkCurrent__text--hover: #333;
$localNavigation__linkCurrent__icon--hover: #666;

$localNavigation__separateBorder--top: #CCCCCC; //essential
$localNavigation__separateBorder--bottom: #ccc;

/* Breadcrumbs */
$breadcrumb__link: #666;
$breadcrumb__link--hover: #999;
$breadcrumb__link--visited: #666;
$breadcrumb__separateBorder--top: #CCCCCC; //essential
$breadcrumb__separateBorder--bottom: #ccc;
$breadcrumb__icon: #999;

/* Contents Area */
$contents__bg: #fff;
$contents__text: #592404; //essential
$contents__icon: #592404; //essential
$contents__icon--reverse: #CCC;
$contents__border: #CCC;
$contents__heading__text: #592404; //essential
$contents__heading__border: #592404; //essential

/* Link */
$text__link--01: #06A;
$text__link--01--hover: #007BCD;
$text__link--01--visited: #527;

$text__link--02: #66C2FF;
$text__link--02--hover: #CCEBFF;
$text__link--02--visited: #EBCCFF;

/* Button */
$button__bg--01: #592404; //essential
$button__text--01: #FFF;
$button__border--01: #592404; //essential
$button__shadowVisible--01: true;

$button__bg--02: #E02D00;
$button__text--02: #FFF;
$button__border--02: #E02D00;
$button__shadowVisible--02: true;

$button__bg--03: #666;
$button__text--03: #FFF;
$button__border--03: #666;
$button__shadowVisible--03: true;

$button__bg--04: #F1F1F1;
$button__text--04: #333;
$button__border--04: #F1F1F1;
$button__shadowVisible--04: true;

$button__bg--05: transparent;
$button__text--05: #0066AA;
$button__border--05: #0066AA;
$button__shadowVisible--05: false;

$button__bg--06: transparent;
$button__text--06: #E02D00;
$button__border--06: #E02D00;
$button__shadowVisible--06: false;

$button__bg--07: transparent;
$button__text--07: #333;
$button__border--07: #333;
$button__shadowVisible--07: false;

/* Accordion */
$accordion__title__bg: #592404; //essential
$accordion__title__text: #fff; //essential
$accordion__title__border: #CCC;

$accordion__content__bg: #FFF;
$accordion__content__text: #592404; //essential
$accordion__content__border: #ffffff; //essential

/* TileLink */
$tileLink__bg: #FFF;
$tileLink__border: #cfbfb2; //essential
$tileLink__text: #592404; //essential

$tileLink__bg--reverse: #ffffff; //essential
$tileLink__border--reverse: #cfbfb2; //essential
$tileLink__text--reverse: #592404;//

/* NewsIndex */
$newsIndex__text: #333;
$newsIndex__border: #EEE;

/* TelephoneCallItem */
$telCall__border--sm: #999;

/* FullwidthVisuals&Carousel */
$carousel__mainText: #592404; //essential
$carousel__subText: #333;
$carousel__subText--sm: #333;
$carousel__arrow__icon: #FFF;
$carousel__indicator__icon: #DBBB48; //essential
$carousel__icon--sm: #DBBB48; //essential
$carousel__arrow__icon__bg: rgba(#000, .5);

$carousel__button__bg--sm: #592404; //essential
$carousel__button__text--sm: #FFF;
$carousel__button__border--sm: #592404; //essential
$carousel__button__shadowVisible--sm: true;

/* Label/Tag */
$labelTag__bg-01: #757575;
$labelTag__text-01: #FFF;

$labelTag__bg-02: #9C27B0;
$labelTag__text-02: #FFF;

$labelTag__bg-03: #3F51B5;
$labelTag__text-03: #FFF;

$labelTag__bg-04: #039BE5;
$labelTag__text-04: #FFF;

$labelTag__bg-05: #43A047;
$labelTag__text-05: #FFF;

$labelTag__bg-06: #FFEB3B;
$labelTag__text-06: #333;

$labelTag__bg-07: #F44336;
$labelTag__text-07: #FFF;

/* DefinitionList */
$definitionList__label__bg-01: #999;
$definitionList__label__text-01: #fff;

$definitionList__label__bg-02: #fff;
$definitionList__label__text-02: #444;

/* Brand Footer */
$brandFooter__bg-01: #666;
$brandFooter__bg-02: #eee;
$brandFooter__bg--open: #ccc;
$brandFooter__text-01: #333;
$brandFooter__text-02: #eee;
$brandFooter__icon-01: #333;
$brandFooter__icon-02: #eee;
$brandFooter__border-01: #ccc;
$brandFooter__border-02: #eee;
$brandFooter__separateBorder--top: #666666; //essential
$brandFooter__separateBorder--bottom: #666666; //essential

/* Page Top */
$pageTop__bg: rgba(102,102,102,.5);
$pageTop__icon: #fff;

/* Option */
$option__bg--01: #FFF;
$option__bg--02: #F1F1F1;
$option__bg--03: #EEE;
$option__bg--04: #ffebf6;//
$option__bg--05: #f7eee9; //essential
$option__bg--06: #ffeee0;//
$option__bg--07: #eaf8e9;//

$option__border--01: #CCC;
$option__border--02: #FFF;
$option__border--03: #F1F1F1;
$option__border--04: #EEE;
$option__border--05: #999;
$option__border--06: #666;
$option__border--07: #cfbfb2;//

$option__inPlaceEditor--01: #FFF;
$option__inPlaceEditor--02: #F1F1F1;
$option__inPlaceEditor--03: #EEE;
$option__inPlaceEditor--04: #CCC;
$option__inPlaceEditor--05: #999;
$option__inPlaceEditor--06: #666;
$option__inPlaceEditor--07: #333;
$option__inPlaceEditor--08: #000;
$option__inPlaceEditor--09: #00B48C;
$option__inPlaceEditor--10: #FF6600;
