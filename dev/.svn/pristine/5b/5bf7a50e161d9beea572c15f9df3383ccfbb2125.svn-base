//
// 共通セクションタイトル
//
.sofina-sectionTitle {
  text-align: center;
  //font-family: $fontFamily_Title;
  //タイトル
  .l-HeadingTitle {
    margin-bottom: 20px;
    //font-family: $fontFamily_Title;
    letter-spacing: 0.05em;
  }
  .l-HeadingTitle__titleBlock__titleBox {
    padding-bottom: 10px;
    .cmn-richtext {
      @include fontSize(lv2_1);
      color: $colorGray_2;
    }
  }
  .l-HeadingTitle__h2 {
    font-size: 0;
    .cmn-richtext {
      @include fontSize(lv7);
      color: #888;
      font-weight: normal;
      font-family: $fontFamily_Title;
      @include bp(lg) {
        letter-spacing: 0.05em;
      }
      @include bp(md) {
        letter-spacing: 0.05em;
      }
      @include bp(sm) {
        letter-spacing: 0.05em;
      }
    }
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    font-family: $fontFamily_Gothic;
    .l-HeadingTitle__titleBlock__titleBox {
      padding-bottom: 10px;
    }
    .l-Text {
      margin-bottom: 4px;
    }
    .l-HeadingTitle__h2 {
      //font-family: $fontFamily_Title;
    }
  }
}

.sofina-sectionTitle_small {
  @include fontSize(lv5); //14px
}

//
// 共通ブロックタイトル　青色
//
.sofina-blocktitle_blue {
  margin-bottom: 0;
  .l-HeadingTitle__titleBlock__titleBox {
    overflow: hidden;
  }
  .l-Text {
    display: inline-block;
    font-family: 'EB Garamond', serif;
    color: $colorIcon_dark;
    @include fontSize(lv4);
    margin-right: 5px;
    letter-spacing: .08em;
  }
  .l-HeadingTitle__titleBlock__titleBox__title {
    display: inline-block;
  }
  .l-HeadingTitle__h2 {
    @include fontSize(lv7);
    color: $colorGray_3;
    font-weight: normal;
  }
}

//
// 共通タイプパネル
//
.sofina-typepanel {
  display: inline-block;
  background-color: #ededed;
  color: #666;
  padding: 4px 8px 4px 8px;
  margin: 1px;
  text-align: center;
  @include fontSize(caption);
}

//
// ボタン
//
//ワクつきアロー
%btn-block-bullet {
  a {
    position: relative;
    border: solid 1px $colorGray_5;
    padding: 5px 10px 5px 0px;
    // background-color: $colorGray_8;
    @include fontSize(lv7);
    text-decoration: none;
    color: $colorGray_3;
    &:hover {
      background-color: #f5f5f5;
      @include opacity(1);
    }
  }

  .cmn-richtext {
    @include bg_main("bullet_glay_right.png");
    background-position: 10px 50%;
    padding-left: 22px;
  }
  //ウィンドウアイコン
  &.icon-linkwindow {
    a {
      .cmn-richtext {
        position: relative;
        padding-right: 25px;
        &:after {
          content: "";
          position: absolute;
          @include img_main("icon_window.png");
          @include vMiddle();
          right: 10px;
        }
      }
    }
  }
  //電話発信アイコン
  &.icon-phonecall {
    a {
      .cmn-richtext {
        span {
          @include bg_main("icon-phonecall@2x.gif", 2);
          background-position: 100% 50%;
          padding-right: 54px;
        }
      }
    }
  }
  &.icon-bullet-gray-left {
    .cmn-richtext {
      @include bg_main("bullet_glay_left.png");
      background-position: 10px 50%;
      padding-left: 22px;
    }
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    .cmn-richtext {
      @include bg_main("bullet_glay_right@2x.png", 2);
    }
    &.icon-bullet-gray-left {
      .cmn-richtext {
        @include bg_main("bullet_glay_left@2x.png", 2);
      }
    }
  }
}

//ワクつきクローズ
%btn-block-close {
  a {
    position: relative;
    border: solid 1px $colorGray_5;
    padding: 5px 15px 5px 5px;
    // background-color: $colorGray_8;
    @include fontSize(lv7);
    text-decoration: none;
    color: $colorGray_3;
    &:hover {
      background-color: #f5f5f5;
      @include opacity(1);
    }
  }

  .cmn-richtext {
    @include bg_main("icon-close.png");
    background-position: 10px 50%;
    padding-left: 26px;
  }
}

//ワクなしグレーアロー
%btn-bullet-glay {
  a {
    padding: 5px 0px 5px 0px;

    @include fontSize(lv7);
    text-decoration: none;
    color: $colorGray_3;
    vertical-align: middle;
    // @include bg_main("bullet_glay_right.png");
    // background-position: 10px 50%;

    &:hover {
      @include opacity(0.8);
    }
    .cmn-richtext {

      &:before {
        content: "";
        @include img_main("bullet_glay_right.png");
        display: inline-block;
        vertical-align: middle;
        margin-right: 3px;
      }
    }

  }
  //PDFアイコン
  &.icon-pdf {
    a {
      .cmn-richtext {

        &:after {

          content: "";
          @include img_main("icon-pdf.png");
          display: inline-block;
          vertical-align: bottom;
          margin-left: 3px;
        }
      }
    }
  }
  //ウィンドウアイコン
  &.icon-linkwindow {
    a {
      .cmn-richtext {
        position: relative;
        display: inline-block;
        padding-right: 15px;
        // padding-right: 20px;
        &:after {
          content: "";
          position: absolute;
          @include img_main("icon_window.png");
          @include vMiddle();
          right: 0px;
        }
      }
    }
  }
  //電話発信アイコン
  &.icon-phonecall {
    a {
      span {
        @include bg_main("icon-phonecall@2x.gif", 2);
        background-position: 100% 50%;
      }
    }
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    a {

      .cmn-richtext {

        &:before {
          @include bg_main("bullet_glay_right@2x.png", 2);
          display: inline-block;
          vertical-align: middle;
          margin-right: 0;
        }
      }
    }
    //PDFアイコン
    &.icon-pdf {
      a {
        .cmn-richtext {
          &:after {
            content: "";
            @include img_main("icon-pdf@2x.png", 2);
            display: inline-block;
            vertical-align: middle;

          }
        }
      }
    }
  }
}

.is-btn {
  //ワクつきグレー
  &.btn-block-bullet-glay {
    @extend %btn-block-bullet;
    a {
      background-color: $colorGray_8;
    }
  }
  //ワクなしグレーアロー
  &.btn-bullet-glay {
    @extend %btn-bullet-glay;
  }
  //ワクつきグレーアロー（背景ダーク）
  &.btn-darkblock-bullet-glay {
    @extend %btn-block-bullet;
    a {
      background-color: #ededed;
      &:hover {
        background-color: $colorGray_5;
        @include opacity(1);
      }
    }
  }
  //ワクなしグレーアロー（背景ダーク）
  &.btn-dark-bullet-glay {
    @extend %btn-block-bullet;

    a {
      border: none;
      background-color: #ededed;
      &:hover {
        background-color: $colorGray_5;
        @include opacity(1);
      }
    }
  }
  //ワクつきホワイト
  &.btn-block-close-white {
    @extend %btn-block-close;
    a {
      background-color: #FFF;
    }
  }
}


