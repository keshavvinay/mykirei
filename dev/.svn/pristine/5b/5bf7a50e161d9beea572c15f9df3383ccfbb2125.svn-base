.sofina-section_shopinfo_big__contholder__info_telnum {
  @include fontSize(lv4);
  font-weight: normal;
}

.sofina-section_shopinfo_big__contholder__info__tel {
  td {
    display: none;
    width: 100%;
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    td {
      display: table-cell;
      width: 100%;
    }
  }
}

.sofina-section_shopinfo_big__contholder__map {
  margin-top: 30px;
  text-align: center;
  .l-Image {
    margin-bottom: 0;
  }
  .l-Image__img {
    max-width: 600px;
    margin: 0px auto 0px auto;
  }
  .btn-map{
    margin-bottom: 0;
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    display: block;
    width: auto;
    text-align: left;
    margin-top: 0px;
    .l-Image__img {
      max-width: inherit;
      float: none;
    }
    .btn-map {
      .cmn-richtext{
        @include fontSize(caption); //12px
      }
    }
  }
}

.sofina-section_shopinfo_big__contholder__map__gmap {
  iframe {
    display: block;
    width: 100%;
    max-width: 640px;
    height: 480px;
    margin: 0px auto 0px auto;
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    iframe {
      height: 200px;
    }
  }
}

.is-btn.sofina-section_shopinfo_big__contholder__map__print {
  a {
    width: 320px;
    padding-top: 15px;
    padding-bottom: 15px;
    @include fontSize(lv4);
    margin-top: 10px;
  }
}

