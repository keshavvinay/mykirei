.sofina-section_shopinfo__contholder {
  display: table;
  width: 100%;
  padding: 30px 30px 30px 30px;
  border: solid 1px $colorGray_5;
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    padding: 20px 10px 10px 10px;
  }
}

.sofina-section_shopinfo__contholder__info {
  display: table-cell;
  width: 60%;
  table {
    margin-top: 20px;
  }
  td {
    vertical-align: top;
    padding-right: 2em;
    padding-bottom: 5px;
    @include fontSize(lv6); //16px
    line-height: 1.5;
    font-weight: normal;
