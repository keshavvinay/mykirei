//
// ワークショップ一覧
//
// SOFINA iPは、11/13発売開始のため、
// それまで非表示としてくださいとのこと
//
////////////////////////////////////
.sofina-section_workshoplist {
  display: none;
  padding-bottom: 100px;
  .g-Section__inner {
    padding-top: 0;
  }
  .g-Column__inner {
    margin: 0px 50px 0px 50px;
  }
  .l-LinkButton {
    position: absolute;
    right: 30px;
    @include vMiddle();
    a {
      padding-right: 0;
    }
  }
  .cmn-richtext {
    padding-left: 0;
  }
  /*-----------------------*/
  // md
  /*-----------------------*/
  @include bp(md) {
    .g-Column__inner {
      margin-right: 0px;
      margin-left: 0px;
    }
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    padding-bottom: 50px;
    font-family: $fontFamily_Gothic;
    .g-Column__inner {
      margin: 0px;
    }
    .l-HeadingTitle {
      padding: 10px 0px 10px 0px;
    }
    .l-LinkButton {
      right: 0;
    }
  }
}

.sofina-section_workshoplist__head {
  position: relative;
  .l-HeadingTitle {
    border-top: solid 1px $colorGray_5;
    border-bottom: solid 1px $colorGray_5;
    padding: 10px 30px 10px 30px;
    margin-bottom: 0;
  }
  .l-HeadingTitle__h2 {
    .cmn-richtext {
      @include fontSize(lv5); //18px
      color: $colorGray_2;
      font-weight: normal;
    }
  }
  .l-LinkButton {
    a {
      color: $colorGray_2;
    }
  }
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    .l-HeadingTitle {
      padding-left: 0;
      padding-right: 0;
    }
  }
}


