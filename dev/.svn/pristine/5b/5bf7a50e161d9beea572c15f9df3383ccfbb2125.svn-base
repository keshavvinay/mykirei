.sofina-section_news__list,
.sofina-workshoplist__list {

  .sofina-whatsnew__line {
    display: block;
    position: relative;
    border-bottom: dotted 1px $colorGray_5;
    padding: 15px 60px 15px 30px;
    text-decoration: none;
    color: $colorGray_2;
    @include fontSize(lv7);

  }
  a {

    &:hover {
      background-color: #f5f5f5;
    }
    &:after {
      content: "&gt";
      position: absolute;
      right: 30px;
      @include vMiddle();
      $url: "arrow_glay_right.png";
      @include img_bps($url);
    }
  }


  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    .sofina-whatsnew__line{
      @include fontSize(caption);
      padding: 10px 15px 10px 0px;
    }
    a {

      &:after {
        right: 0;
      }
    }
  }
}

//ニュース一個のヘッド
.sofina-section_news__list__head {
  display: table-cell;
  width: 180px;
  vertical-align: middle;
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    display: block;
  }
}

//タイプ
.sofina-section_news__list__head__type {
  display: inline-block;
  margin-left: 1em;
  border: solid 1px $colorGray_5;
  padding: 4px 0px 4px 0px;
  width: 77px;
  text-align: center;
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    padding: 2px 0px 2px 0px;
  }
}
a:visited {
  .sofina-section_news__list__head__type {
    border: solid 1px $colorGray_5;
  }
}

//ニュース一個のボディ
.sofina-section_news__list__body {
  display: table-cell;
  vertical-align: middle;
  line-height: 1.5;
  /*-----------------------*/
  // sm
  /*-----------------------*/
  @include bp(sm) {
    display: block;
    padding-top: 10px;
  }
}

