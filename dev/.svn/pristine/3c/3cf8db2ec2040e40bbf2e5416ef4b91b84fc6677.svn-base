@charset "utf-8";

/*****************************
  Content Color set (f)
 *****************************/

/* Body */
$body__bg: #FFFFFF;

/* Brand Header */
$brandHeader__bg: #ADD4F4;
$brandHeader__text: #FFFFFF;
$brandHeader__icon: #FFFFFF;
$brandHeader__border: #FFFFFF;
$brandHeader__button__bg: #FFFFFF;
$brandHeader__button__icontext: #63B2E2;

$brandHeader__linkNormal__bg: transparent;
$brandHeader__linkNormal__text: #FFFFFF;
$brandHeader__linkNormal__icon: #FFFFFF;
$brandHeader__linkNormal__bg--hover: #63B2E3;
$brandHeader__linkNormal__text--hover: #FFFFFF;
$brandHeader__linkNormal__icon--hover: #FFFFFF;

$brandHeader__linkCurrent__bg: #E5F3F7;
$brandHeader__linkCurrent__text: #ADD4F4;
$brandHeader__linkCurrent__icon: #FFFFFF;
$brandHeader__linkCurrent__bg--hover: #63B2E3;
$brandHeader__linkCurrent__text--hover: #FFFFFF;
$brandHeader__linkCurrent__icon--hover: #FFFFFF;

$brandHeader__separateBorder--top: #FFFFFF;
$brandHeader__separateBorder--bottom: #CCCCCC;

/* Local Navigation */
$localNavigation__bg: #F1F1F1;
$localNavigation__bg--smmd: #ADD4F4;
$localNavigation__text: #333333;
