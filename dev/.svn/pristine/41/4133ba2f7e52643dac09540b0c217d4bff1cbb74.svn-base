@charset "utf-8";


/* TwitterPost ================================================== */
.g-TwitterPost {
  &.is-colorset--1 {
    color: $twitterPost__text--01;
    .cmn-icon {
      color: $twitterPost__icon--01;
    }
    .g-TwitterPostUnit {
      border-color: $twitterPost__border--01;
    }
    .g-TwitterPost__contentsBlock__links__item__link,
    .g-TwitterPost__contentsBlock__visit__link,
    .g-TwitterPost__contentsBlock__tweet a {
      color: $twitterPost__link--01;
      &:link {
        color: $twitterPost__link--01;
      }
      &:visited {
        color: $twitterPost__link--01--visited;
      }
      &:hover,
      &:focus,
      &:active {
        color: $twitterPost__link--01--hover;
      }
    }
  }
  &.is-colorset--2 {
    color: $twitterPost__text--02;
    .cmn-icon {
      color: $twitterPost__icon--02;
    }
    .g-TwitterPostUnit {
      border-color: $twitterPost__border--02;
    }
    .g-TwitterPost__contentsBlock__links__item__link,
    .g-TwitterPost__contentsBlock__visit__link,
    .g-TwitterPost__contentsBlock__tweet a {
      color: $twitterPost__link--02;
      &:link {
        color: $twitterPost__link--02;
      }
      &:visited {
        color: $twitterPost__link--02--visited;
      }
      &:hover,
      &:focus,
      &:active {
        color: $twitterPost__link--02--hover;
      }
    }
  }
}
