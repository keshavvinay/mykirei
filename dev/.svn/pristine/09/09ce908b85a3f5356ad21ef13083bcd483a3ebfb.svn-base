$option__border--06: #4CA3C3;
$option__border--07: #333333;

$option__inPlaceEditor--01: #FFFFFF;
$option__inPlaceEditor--02: #EC9100;
$option__inPlaceEditor--03: #F5F5F5;
$option__inPlaceEditor--04: #F2F2F2;
$option__inPlaceEditor--05: #DDDDDD;
$option__inPlaceEditor--06: #777777;
$option__inPlaceEditor--07: #333333;
$option__inPlaceEditor--08: #007CAA;
$option__inPlaceEditor--09: #4CA3C3;
$option__inPlaceEditor--10: #9AC4D4;
