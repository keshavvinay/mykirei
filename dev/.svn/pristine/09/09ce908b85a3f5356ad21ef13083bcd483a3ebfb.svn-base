@charset "utf-8";

/*****************************
  Content Color set (sofina)
 *****************************/

/* Body */
$body__bg: #FFFFFF;

/* Brand Header */
$brandHeader__bg: #FFFFFF;
$brandHeader__text: #333333;
$brandHeader__icon: #999999;
$brandHeader__border: #CCCCCC;
$brandHeader__button__bg: #666666;
$brandHeader__button__icontext: #FFFFFF;

$brandHeader__linkNormal__bg: transparent;
$brandHeader__linkNormal__text: #333333;
$brandHeader__linkNormal__icon: #999999;
$brandHeader__linkNormal__bg--hover: #EEEEEE;
$brandHeader__linkNormal__text--hover: #333333;
$brandHeader__linkNormal__icon--hover: #666666;

$brandHeader__linkCurrent__bg: #EEEEEE;
$brandHeader__linkCurrent__text: #333333;
$brandHeader__linkCurrent__icon: #666666;
$brandHeader__linkCurrent__bg--hover: #CCCCCC;
$brandHeader__linkCurrent__text--hover: #333333;
$brandHeader__linkCurrent__icon--hover: #666666;

$brandHeader__separateBorder--top: #CCCCCC;
$brandHeader__separateBorder--bottom: #CCCCCC;

/* Local Navigation */
$localNavigation__bg: #F1F1F1;
$localNavigation__bg--smmd: #FFFFFF;
$localNavigation__text: #333333;
$localNavigation__icon: #999999;
$localNavigation__border: #CCCCCC;

$localNavigation__linkNormal__bg: transparent;
$localNavigation__linkNormal__text: #333333;
$localNavigation__linkNormal__icon: #999999;
$localNavigation__linkNormal__bg--hover: #CCCCCC;
$localNavigation__linkNormal__text--hover: #333333;
$localNavigation__linkNormal__icon--hover: #666666;

$localNavigation__linkCurrent__bg: #CCCCCC;
$localNavigation__linkCurrent__text: #333333;
$localNavigation__linkCurrent__icon: #666666;
$localNavigation__linkCurrent__bg--hover: #AAAAAA;
$localNavigation__linkCurrent__text--hover: #333333;
$localNavigation__linkCurrent__icon--hover: #666666;

$localNavigation__separateBorder--top: transparent;
$localNavigation__separateBorder--bottom: #CCCCCC;

/* Breadcrumbs */
$breadcrumb__link: #666666;
$breadcrumb__link--hover: #999999;
$breadcrumb__link--visited: #666666;
$breadcrumb__separateBorder--top: transparent;
$breadcrumb__separateBorder--bottom: #CCCCCC;
$breadcrumb__icon: #999999;

/* Contents Area */
$contents__bg: #FFFFFF;
$contents__text: #333333;
$contents__icon: #007CAA;
$contents__icon--reverse: #FFFFFF;
$contents__border: #DDDDDD;
$contents__heading__text: #333333;
$contents__heading__border: transparent;

/* Link */
$text__link--01: #333333;
$text__link--01--hover: #777777;
$text__link--01--visited: #333333;

$text__link--02: #007CAA;
$text__link--02--hover: #4CA3C3;
$text__link--02--visited: #007CAA;

/* Button */
$button__bg--01: #FFFFFF;
$button__text--01: #333333;
$button__border--01: #DDDDDD;
$button__shadowVisible--01: false;

$button__bg--02: #F9F9F9;
$button__text--02: #333333;
$button__border--02: #DDDDDD;
$button__shadowVisible--02: false;

$button__bg--03: #666666;
$button__text--03: #FFFFFF;
$button__border--03: #666666;
$button__shadowVisible--03: false;

$button__bg--04: #F1F1F1;
$button__text--04: #333333;
$button__border--04: #F1F1F1;
$button__shadowVisible--04: false;

$button__bg--05: #FFFFFF;
$button__text--05: #007CAA;
$button__border--05: #DDDDDD;
$button__shadowVisible--05: false;

$button__bg--06: #FFFFFF;
$button__text--06: #EC9100;
$button__border--06: #DDDDDD;
$button__shadowVisible--06: false;

$button__bg--07: transparent;
$button__text--07: #333333;
$button__border--07: #333333;
$button__shadowVisible--07: false;

/* Accordion */
$accordion__title__bg: #F5F5F5;
$accordion__title__text: #333333;
$accordion__title__border: #DDDDDD;

$accordion__content__bg: #FFFFFF;
$accordion__content__text: #333333;
$accordion__content__border: #DDDDDD;

/* TileLink */
$tileLink__bg: #FFFFFF;
$tileLink__border: #DDDDDD;
$tileLink__text: #333333;

$tileLink__bg--reverse: #F7FEFE;
$tileLink__border--reverse: #F7FEFE;
$tileLink__text--reverse: #333333;

/* NewsIndex */
$newsIndex__text: #333333;
$newsIndex__border: #DDDDDD;

/* TelephoneCallItem */
$telCall__border--sm: #777777;

/* FullwidthVisuals&Carousel */
$carousel__mainText: #333333;
$carousel__subText: #333333;
$carousel__subText--sm: #333333;
$carousel__arrow__icon: #007CAA;
$carousel__indicator__icon: #9AC4D4;
$carousel__icon--sm: #9AC4D4;
$carousel__arrow__icon__bg: rgba(#FFFFFF, .5);

$carousel__button__bg--sm: #FFFFFF;
$carousel__button__text--sm: #333333;
$carousel__button__border--sm: #DDDDDD;
$carousel__button__shadowVisible--sm: false;

/* Label/Tag */
$labelTag__bg-01: #FFFFFF;
$labelTag__text-01: #333333;

$labelTag__bg-02: #F5F5F5;
$labelTag__text-02: #333333;

$labelTag__bg-03: #777777;
$labelTag__text-03: #FFFFFF;

$labelTag__bg-04: #039BE5;
$labelTag__text-04: #FFFFFF;

$labelTag__bg-05: #43A047;
$labelTag__text-05: #FFFFFF;

$labelTag__bg-06: #FFEB3B;
$labelTag__text-06: #333333;

$labelTag__bg-07: #F44336;
$labelTag__text-07: #FFFFFF;

/* DefinitionList */
$definitionList__label__bg-01: #999999;
$definitionList__label__text-01: #FFFFFF;

$definitionList__label__bg-02: #FFFFFF;
$definitionList__label__text-02: #444444;

/* Brand Footer */
$brandFooter__bg-01: #F9F9F9;
$brandFooter__bg-02: #F2F2F2;
$brandFooter__bg--open: #F9F9F9;
$brandFooter__text-01: #333333;
$brandFooter__text-02: #333333;
$brandFooter__icon-01: #007CAA;
$brandFooter__icon-02: #007CAA;
$brandFooter__border-01: #DDDDDD;
$brandFooter__border-02: #DDDDDD;
$brandFooter__separateBorder--top: #F9F9F9;
$brandFooter__separateBorder--bottom: #F9F9F9;

/* Page Top */
$pageTop__bg: rgba(#007CAA, .5);
$pageTop__icon: #FFFFFF;

/* Option */
$option__bg--01: #FFFFFF;
$option__bg--02: #F9F9F9;
$option__bg--03: #E3F8F8;
$option__bg--04: #F7FEFE;
