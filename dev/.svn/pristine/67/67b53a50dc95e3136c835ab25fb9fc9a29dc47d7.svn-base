body {
  margin: 0;
  font-family: "ProximaNova-Regular", Arial, sans-serif;
}
body .g-Area#area-BrHeader {
  padding-top: 66px;
}
@media (max-width: 640px) {
  body .g-Area#area-BrHeader {
    padding-top: 50px;
  }
}

button {
  outline: none !important;
}

a {
  text-decoration: none;
  color: #22244a;
}

a:hover {
  transition: 0.2s;
}

h1 sup {
  top: -30px;
}
@media (max-width: 641px) {
  h1 sup {
    top: -25px;
  }
}

.is-rightIcon .cmn-richtext {
  padding-right: 0 !important;
}

.is-leftIcon .cmn-richtext {
  padding-left: 0 !important;
}

.cmn-icon {
  display: none !important;
}
.cmn-icon::before {
  display: none !important;
}

.utility-wrapper a {
  font-family: ProximaNova-Regular;
}
.utility-wrapper a:hover {
  font-weight: bold;
}

.float-right {
  float: right;
}

.float-left {
  float: left;
}

.full-width {
  width: 100% !important;
}

.biore-header-wrapper {
  background-image: url(/content/dam/sites/kaousa/www-biore-com/master/backgrounds/biore-header-bg.jpg);
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  z-index: 100;
  transition: 0.5s;
  position: fixed;
  top: 0;
  width: 100%;
  box-shadow: 0px 5px 6px -3px #5d5d5d;
  /*Search icon*/
}
.biore-header-wrapper .header {
  min-height: 66px;
  margin: 0 auto;
  position: relative;
  max-width: 1366px;
  margin: 0 auto;
}
@media (max-width: 640px) {
  .biore-header-wrapper .header {
    min-height: 50px;
  }
}
.biore-header-wrapper .logo-wrapper {
  display: inline-block;
  position: absolute;
  top: 0;
  left: 50px;
  overflow: hidden;
  z-index: 10;
}
@media (max-width: 640px) {
  .biore-header-wrapper .logo-wrapper {
    left: 0;
  }
}
.biore-header-wrapper .logo-wrapper img {
  max-width: 150px;
  top: -30px;
  position: relative;
}
@media (max-width: 1024px) {
  .biore-header-wrapper .logo-wrapper img {
    max-width: 130px;
    top: -25px;
  }
}
@media (max-width: 640px) {
  .biore-header-wrapper .logo-wrapper img {
    max-width: 90px;
    top: -18px;
  }
}
.biore-header-wrapper .main-nav-ul {
  list-style: none;
  margin: 0;
  padding-left: 210px;
}
@media (min-width: 992px) and (max-width: 1024px) {
  .biore-header-wrapper .main-nav-ul {
    padding-left: 150px;
  }
}
@media (max-width: 991px) {
  .biore-header-wrapper .main-nav-ul {
    padding: 0;
  }
}
.biore-header-wrapper .main-nav-ul .g-TextLinkP {
  margin-bottom: 0;
}
.biore-header-wrapper .main-nav-ul .language-toggle ul li a.g-TextLinkUnit__link.on {
  border-bottom: none;
}
.biore-header-wrapper .main-nav-ul > li {
  padding: 1.25rem;
  display: inline-block;
}
.biore-header-wrapper .main-nav-ul > li > .g-TextLinkP a.g-TextLinkUnit__link {
  color: #ffffff;
  padding-bottom: 2px;
  text-transform: uppercase;
  border-bottom: 2px solid transparent;
  transition: 0.3s ease border;
}
@media (max-width: 991px) {
  .biore-header-wrapper .main-nav-ul > li > .g-TextLinkP a.g-TextLinkUnit__link {
    color: #304065;
    border: 0;
    padding: 0;
    font-family: "ProximaNova-Semibold";
    font-weight: bold;
  }
}
.biore-header-wrapper .main-nav-ul > li > .g-TextLinkP a.g-TextLinkUnit__link.on {
  border-bottom-color: #ec008d;
}
@media (min-width: 992px) and (max-width: 1024px) {
  .biore-header-wrapper .main-nav-ul > li {
    padding: 1.25rem 1rem;
  }
}
@media (max-width: 991px) {
  .biore-header-wrapper .main-nav-ul > li {
    padding: 0.5rem;
    display: block;
    border-bottom: 1px solid #c2c2c2;
  }
  .biore-header-wrapper .main-nav-ul > li:last-child {
    border-bottom: 0;
  }
}
@media (max-width: 1024px) {
  .biore-header-wrapper .main-nav-ul #products .g-TextLinkP {
    display: inline-block;
  }
}
@media (min-width: 1025px) {
  .biore-header-wrapper .main-nav-ul #products:hover .header-dropdown-wrapper {
    visibility: visible;
    opacity: 1;
  }
  .biore-header-wrapper .main-nav-ul #products:hover .g-TextLinkP a.g-TextLinkUnit__link {
    border-bottom-color: #ec008d;
  }
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper {
  position: absolute;
  visibility: hidden;
  transition: 0.3s ease opacity;
  opacity: 0;
  left: 0;
  right: 0;
  /*margin-left: -50px;
  margin-right: -50px;*/
  top: 100%;
  background-color: #fff;
  box-shadow: 0px 5px 6px -3px #5d5d5d;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper.on {
  visibility: visible;
  opacity: 1;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .header-dropdown-inner {
  display: flex;
  padding-left: 245px;
  padding-top: 1.5rem;
  padding-bottom: 1.5rem;
  box-shadow: inset 0px 5px 6px -3px #5d5d5d;
}
@media (max-width: 1024px) {
  .biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .header-dropdown-inner {
    padding: 1.5rem;
    padding-top: 2rem;
  }
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .header-dropdown-inner .g-Image img {
  margin-left: 0;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .g-ListP {
  margin-bottom: 1.5rem;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .g-ListP__ul {
  padding: 0;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .g-ListP__ul a {
  font-size: 14px;
  color: #c2c2c2;
  text-transform: capitalize;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .g-ListP__ul .g-ListP__ul__item {
  margin-bottom: 0.5rem;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .g-ListP__ul .cmn-icon {
  display: none;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper a.g-TextLinkUnit__link {
  font-size: 1.4rem;
  font-family: "LubalinGraphStd-Demi";
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .is-gutter--m.g-TextLinkP {
  margin-bottom: 0.625rem;
}
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .col-1,
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .col-2,
.biore-header-wrapper .main-nav-ul .header-dropdown-wrapper .col-3 {
  /*float: left;*/
  padding-right: 3rem;
}
@media (max-width: 991px) {
  .biore-header-wrapper .ul-wrapper {
    position: absolute;
    visibility: hidden;
    opacity: 0;
    top: 100%;
    min-height: 70vh;
    max-height: 80vh;
    overflow: auto;
    right: 0;
    width: 50%;
    background-color: #ffffff;
    z-index: 10;
    box-shadow: -1px 2px 8px 0px #5d5d5d;
    padding: 1.2rem;
  }
  .biore-header-wrapper .ul-wrapper.open {
    visibility: visible;
    opacity: 1;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper {
    display: none;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-ListP {
    display: none;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-ListP ul {
    padding: 0;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-ListP li {
    padding: 0.5rem;
    border-bottom: 1px solid #c2c2c2;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-ListP a {
    color: #c2c2c2;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-TextLinkP {
    margin-bottom: 0;
    padding: 0.5rem;
    cursor: pointer;
    border-bottom: 1px solid #c2c2c2;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-TextLinkP ul.g-TextLinkP__list {
    display: inline-block;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-TextLinkP a.g-TextLinkUnit__link {
    font-size: 1rem;
    font-family: "LubalinGraphStd-Demi";
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .header-dropdown-inner a.g-Image {
    margin-top: 2rem;
  }
}
@media (max-width: 640px) {
  .biore-header-wrapper .ul-wrapper {
    width: 70%;
    min-height: inherit;
    max-height: inherit;
    height: 100vh;
  }
  .biore-header-wrapper .ul-wrapper .header-dropdown-wrapper .g-ListP a {
    font-size: 14px;
  }
  .biore-header-wrapper .ul-wrapper.open {
    top: 0;
  }
}
.biore-header-wrapper .icon-search:before {
  content: '\f002';
  font-family: "icomoon";
  padding: 0.625rem;
  width: 20px;
  height: 20px;
  background-color: #ec008d;
  border-radius: 100%;
  vertical-align: middle;
}
.biore-header-wrapper .icon-search {
  cursor: pointer;
  color: #fff;
  display: inline-block;
}
.biore-header-wrapper .bioreSearchBlock {
  position: absolute;
  right: 50px;
  top: 1px;
  width: auto;
  padding: 1.25rem 0;
}
@media (min-width: 992px) and (max-width: 1024px) {
  .biore-header-wrapper .bioreSearchBlock {
    right: 25px;
  }
