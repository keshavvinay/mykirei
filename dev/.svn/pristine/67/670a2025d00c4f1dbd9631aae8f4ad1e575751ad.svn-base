
/* @import must be at top of file, otherwise CSS will not work */

@import url("https://fonts.gstatic.com/s/karma/v9/va9I4kzAzMZRGLBoQeNVkqDO.woff2");

//karmafont family
@font-face {
    font-family: 'Karma';
    src:  url('../fonts/webfonts/karma.woff2') format('woff2');
}

//conduititcstd font family
@font-face {
    font-family: 'ConduitITCStd-Medium';
    src: url('../fonts/webfonts/conduititcstd_med.eot');
    src: url('../fonts/webfonts/conduititcstd_med.ttf') format('truetype'),
         url('../fonts/webfonts/conduititcstd_med.woff2') format('woff2'),
         url('../fonts/webfonts/conduititcstd_med.woff') format('woff');
}

