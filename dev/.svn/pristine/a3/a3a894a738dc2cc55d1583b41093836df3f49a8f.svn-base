.fontList__pagenation__link__icon {
  font-size: 1rem;
  line-height: 1;
  vertical-align: middle;
  padding-bottom: 2px;
}
.fontList__pagenation__link__icon.prev {
  margin-right: 5px;
}
.fontList__pagenation__link__icon.next {
  margin-left: 5px;
}

.fontList__pagenation__item {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-basis: 70px;
}

.fontList__item {
  border-bottom: 1px dotted #ccc;
  list-style: none;
  display: table;
  width: 100%;
  height: 62px;
  vertical-align: middle;
  table-layout: fixed;
}

.fontList__item__name {
  display: table-cell;
  vertical-align: middle;
  width: 33%;
  font-size: 16px;
  font-weight: bold;
  padding-right: 24px;
}

.fontList__item__text {
  display: table-cell;
  vertical-align: middle;
  font-size: 24px;
  padding-right: 24px;
}

.fontList__item__copy {
  position: relative;
  display: table-cell;
  vertical-align: middle;
  margin-left: auto;
  width: 198px;
}

.fontList__item__copyTooltip {
  position: absolute;
  top: -25px;
  left: 0;
  right: 0;
  margin: auto;
  border-radius: 4px;
  display: none;
  width: 140px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  background-color: #000;
  color: #fff;
  z-index: 100;
}
.fontList__item__copyTooltip:after {
  top: 100%;
  left: 50%;
  border: solid transparent;
  content: " ";
  height: 0;
  width: 0;
  position: absolute;
  pointer-events: none;
  border-color: transparent;
  border-top-color: #000000;
  border-width: 15px;
  margin-left: -15px;
}

.section__note {
  border-top: 1px solid #ccc;
  padding: 28px 0 14px;
}

.noteWrapper {
  padding: 0 32px;
}

.flowList {
  padding-left: 30px;
}

.flowList__item__text {
  padding-left: 5px;
}

.anotationList {
  padding: 0;
  padding-left: 8px;
}

.anotationList__item {
  list-style-type: none;
  position: relative;
}
.anotationList__item:before {
  content: '・';
  display: block;
  position: absolute;
}

.anotationList__item__text {
  padding-left: 20px;
}

.iconList {
  padding: 0;
  padding-left: 24px;
}

.iconList__item {
  border-bottom: 1px dotted #ccc;
  list-style: none;
  display: table;
  width: 100%;
  height: 58px;
  vertical-align: middle;
  table-layout: fixed;
}

.iconList__item__image {
  display: table-cell;
  vertical-align: middle;
  text-align: center;
  width: 80px;
}
.iconList__item__image span {
  vertical-align: middle;
  font-size: 29px;
}

.iconList__item__name {
  display: table-cell;
  vertical-align: middle;
  width: 240px;
  padding-right: 24px;
  font-size: 14px;
  font-weight: bold;
}

.iconList__item__fileName {
