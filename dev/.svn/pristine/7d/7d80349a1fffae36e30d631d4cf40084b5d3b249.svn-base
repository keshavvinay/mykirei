.c190__navi {
  display: none;
}
@media only screen and (min-width: 1025px) {
  .c190__navi {
    position: absolute;
    z-index: 2;
    top: 0;
    display: block;
    width: 42%;
    height: 100%;
  }
  .c190__navi.c190__navi--prev {
    left: 0;
  }
  .c190__navi.c190__navi--next {
    right: 0;
  }
  .c190__navi .c190__navi__arrow {
    display: block;
    opacity: 0;
    -webkit-transition: all .4s;
    transition: all .4s;
    width: 54px;
    height: 54px;
    border-width: 2px;
    border-top: solid #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -27px;
    margin-left: -27px;
  }
  .c190__navi .c190__navi__arrow.c190__navi__arrow--prev {
    border-left: solid #fff;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
  }
  .c190__navi .c190__navi__arrow.c190__navi__arrow--next {
    border-right: solid #fff;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
  }
  .c190__navi:hover .c190__navi__arrow {
    opacity: 1;
  }
}

