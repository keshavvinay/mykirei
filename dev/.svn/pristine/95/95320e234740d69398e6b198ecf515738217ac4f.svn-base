$brandHeader__linkNormal__text--hover: #ff6801; //
$brandHeader__linkNormal__icon--hover: #ffffff; //

$brandHeader__linkCurrent__bg: #fff2e6; //
$brandHeader__linkCurrent__text: #ff6801; //
$brandHeader__linkCurrent__icon: #ffffff; //
$brandHeader__linkCurrent__bg--hover: #fff2e6; //
