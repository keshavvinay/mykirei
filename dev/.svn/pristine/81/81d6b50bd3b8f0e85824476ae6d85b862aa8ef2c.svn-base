@charset "utf-8";

/*****************************
  Content Color set (8x4men)
 *****************************/

/* Body */
$body__bg: #FFFFFF;

/* Brand Header */
$brandHeader__bg: #0086D1;
$brandHeader__border: #D9ECF6;
$brandHeader__button__bg: #FFFFFF;
$brandHeader__button__icontext: #0086D1;

$brandHeader__linkNormal__bg: #0086D1;
$brandHeader__linkNormal__text: #FFFFFF;
$brandHeader__linkNormal__icon: #FFFFFF;
$brandHeader__linkNormal__bg--hover: #35A3E0;
$brandHeader__linkNormal__text--hover: #FFFFFF;
$brandHeader__linkNormal__icon--hover: #FFFFFF;

$brandHeader__linkCurrent__bg: #35A3E0;
$brandHeader__linkCurrent__text: #FFFFFF;
$brandHeader__linkCurrent__icon: #FFFFFF;
$brandHeader__linkCurrent__bg--hover: #35A3E0;
$brandHeader__linkCurrent__text--hover: #FFFFFF;
$brandHeader__linkCurrent__icon--hover: #FFFFFF;

$brandHeader__separateBorder--top: #0086D1;
$brandHeader__separateBorder--bottom: #0086D1;

/* Local Navigation */
$localNavigation__bg: #D9ECF6;
$localNavigation__bg--smmd: #FFFFFF;
$localNavigation__border: #FFFFFF;

$localNavigation__linkNormal__bg: transparent;
$localNavigation__linkNormal__text: #0086D1;
$localNavigation__linkNormal__icon: #0086D1;
$localNavigation__linkNormal__bg--hover: #FFFFFF;
$localNavigation__linkNormal__text--hover: #0086D1;
$localNavigation__linkNormal__icon--hover: #0086D1;

$localNavigation__linkCurrent__bg: #FFFFFF;
$localNavigation__linkCurrent__text: #0086D1;
$localNavigation__linkCurrent__icon: #0086D1;
$localNavigation__linkCurrent__bg--hover: #FFFFFF;
$localNavigation__linkCurrent__text--hover: #0086D1;
$localNavigation__linkCurrent__icon--hover: #0086D1;

$localNavigation__separateBorder--top: #D9ECF6;
$localNavigation__separateBorder--bottom: #D9ECF6;

/* Breadcrumbs */
$breadcrumb__link: #666666;
$breadcrumb__link--hover: #999999;
$breadcrumb__link--visited: #999999;
$breadcrumb__separateBorder--bottom: #CCCCCC;
$breadcrumb__icon: #999999;

/* Contents Area */
$contents__bg: #FFFFFF;
$contents__text: #000000;
$contents__icon: #333333;
$contents__icon--reverse: #CCCCCC;
$contents__heading__text: #000000;
$contents__heading__border: #000000;

/* Link */
$text__link--01: #333333;
$text__link--01--hover: #666666;
$text__link--01--visited: #666666;

$text__link--02: #66C2FF;
$text__link--02--hover: #35A3E0;
$text__link--02--visited: #35A3E0;

/* Button */
$button__bg--01: #0086D1;
$button__text--01: #FFFFFF;
$button__border--01: #0086D1;
$button__shadowVisible--01: false;

$button__bg--02: #E4007F;
$button__text--02: #FFFFFF;
$button__border--02: #E4007F;
$button__shadowVisible--02: false;

$button__bg--03: #000000;
$button__text--03: #FFFFFF;
$button__border--03: #000000;
$button__shadowVisible--03: false;

$button__bg--04: #D9ECF6;
$button__text--04: #0086D1;
$button__border--04: #D9ECF6;
$button__shadowVisible--04: false;

$button__bg--05: transparent;
$button__text--05: #0086D1;
$button__border--05: #0086D1;
$button__shadowVisible--05: false;

$button__bg--06: transparent;
$button__text--06: #E4007F;
$button__border--06: #E4007F;
$button__shadowVisible--06: false;

$button__bg--07: transparent;
$button__text--07: #000000;
$button__border--07: #000000;
$button__shadowVisible--07: false;

/* Accordion */
$accordion__title__bg: #F1F1F1;
$accordion__title__text: #000000;
$accordion__title__border: #CCCCCC;

$accordion__content__bg: #FFFFFF;
$accordion__content__text: #000000;
$accordion__content__border: #CCCCCC;

/* TileLink */
$tileLink__bg: #FFFFFF;
$tileLink__border: #0086D1;
$tileLink__text: #0086D1;

$tileLink__bg--reverse: #FFFFFF;
$tileLink__border--reverse: transparent;
$tileLink__text--reverse: #0086D1;

/* NewsIndex */
$newsIndex__text: #000000;
$newsIndex__border: #EEEEEE;

/* TelephoneCallItem */
$telCall__border--sm: #999999;

/* FullwidthVisuals&Carousel */
$carousel__mainText: #000000;
$carousel__subText: #000000;
$carousel__subText--sm: #000000;
$carousel__arrow__icon: #FFFFFF;
$carousel__indicator__icon: #999999;
