@charset "UTF-8";


.method {
  margin-top: 50px;
  padding: 10px;
  background: #CCCCCC;
}
.componentName {
  display: inline-block;
  margin: 5px 0;
  padding: 3px 10px;
  background: #000;
  color: #fff;
  font-size: 1.5em;
}
.settingName {
  margin: 20px 0 10px;
  padding: 3px 10px;
  background: #666;
  color: #fff;
  font-weight: normal;
}
.pageTop {
  position: fixed;
  right: 0;
  bottom: 0;
}
.pageTop a {
  padding: 10px 20px;
  background-color: #333;
  text-align: right;
  text-decoration: none;
  color: #fff;
}
.pageTop a:hover {
  background-color: #555;
}
.backBtn {

}
.menuBox {
  position: fixed;
  right: 0;
  top: 0;
  padding-right: 10px;
  background-color: rgba(0,0,0,.5);
  z-index: 9;
}
.menuBox ul {
  color: #fff;
}
.menuBox a {
  color: #fff;
}
