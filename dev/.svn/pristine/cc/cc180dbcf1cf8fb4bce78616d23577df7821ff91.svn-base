<body>

<!-- area-GlHeader-lg/sm -->
<div id="menu">
  <div class="js-menuHolder">
    <div class="g-GlMenu">

      <div class="g-GlMenu__navBlock">
        <ul class="g-GlMenuNavList">
          <li><a href="http://www.kao.com/jp/corp/">会社情報</a></li>
          <li><a href="http://www.kao.com/jp/products/">製品カタログ</a></li>
          <li><a href="http://www.kao.com/jp/qa/">製品Q&amp;A</a></li>
          <li><a href="http://www.kao.com/jp/soudan/">お問い合わせ</a></li>
        </ul>
      </div>

      <div class="g-GlMenu__utilityBlock">
        <div class="g-GlMenuUtilitySearch">
          <form action="http://crmsearch.kao.com/search" name="gs" method="GET">
            <input type="text" name="q" placeholder="サイト内検索" />
            <button type="submit" name="btnG">検索する<span class="kao-icon kao-icon--search g-GlMenuUtilitySearch__icon"></span></button>
            <!-- <input type="image" name="btnG" class="search-image" src="/common/jp/ja/imgs/search_button_head.gif" alt="" value="&#26908;&#32034;"> -->
            <input type="hidden" name="client" value="test">
            <input type="hidden" name="output" value="xml_no_dtd">
            <input type="hidden" name="proxystylesheet" value="test">
            <input type="hidden" name="sort" value="date:D:L:d1">
            <input type="hidden" name="entqr" value="3">
            <input type="hidden" name="oe" value="UTF-8">
            <input type="hidden" name="ie" value="UTF-8">
            <input type="hidden" name="ud" value="1">
            <input type="hidden" name="site" value="test">
          </form>
        </div>
        <div class="g-GlMenuUtilityRegion">
          <div class="g-GlMenuUtilityRegion__country">
            <p>Japan</p>
          </div>
          <div class="g-GlMenuUtilityRegion__world">
            <p><a href="http://www.kao.com/index_ja.html"><span class="kao-icon kao-icon--xxx g-GlMenuUtilityRegion__world__icon"></span>Kao Worldwide</a></p>
          </div>
        </div>
      </div>

      <div class="g-GlMenu__buttonBlock">
        <div class="g-GlMenuButtonLink"><a href="#">メニューを閉じる<span class="kao-icon kao-icon--close g-GlMenuButtonLink__icon"></span></a></div>
      </div>

    </div>
  </div>
</div>
<!-- / area-GlHeader-lg/sm -->


<div id="wrapper">

<!-- area-GlHeader -->
<div class="g-Area" id="area-GlHeader">
  <div class="g-GlHeader">
  <div class="g-GlHeader__inner">

    <div class="g-GlHeader__logoBlock">
      <div class="g-GlHeaderLogo"><a href="http://www.kao.com/jp/"><img src="/content/dam/sites/kao/inc-common/gl_header_logo.png" alt="kao" /></a></div>
    </div>

    <div class="g-GlHeader__utilityBlock">
      <div class="g-GlHeaderUtilityRegion">
        <div class="g-GlHeaderUtilityRegion__country">
          <p>Japan</p>
        </div>
        <div class="g-GlHeaderUtilityRegion__world">
          <p><a href="http://www.kao.com/index_ja.html"><span class="g-GlHeaderUtilityRegion__world__icon"></span>Kao Worldwide</a></p>
        </div>
      </div>
      <div class="g-GlHeaderUtilitySearch">
        <form action="http://crmsearch.kao.com/search" name="gs" method="GET">
          <input type="text" name="q" placeholder="サイト内検索" />
          <button type="submit" name="btnG">検索する<span class="kao-icon kao-icon--search g-GlHeaderUtilitySearch__icon"></span></button>
          <!-- <input type="image" name="btnG" class="search-image" src="/common/jp/ja/imgs/search_button_head.gif" alt="" value="&#26908;&#32034;"> -->
          <input type="hidden" name="client" value="test">
          <input type="hidden" name="output" value="xml_no_dtd">
          <input type="hidden" name="proxystylesheet" value="test">
          <input type="hidden" name="sort" value="date:D:L:d1">
          <input type="hidden" name="entqr" value="3">
          <input type="hidden" name="oe" value="UTF-8">
          <input type="hidden" name="ie" value="UTF-8">
          <input type="hidden" name="ud" value="1">
          <input type="hidden" name="site" value="test">
        </form>
      </div>
    </div>

    <div class="g-GlHeader__buttonBlock">
      <div class="g-GlHeaderButtonLink"><a href="#">メニューを開く<span class="kao-icon kao-icon--menu g-GlHeaderButtonLink__icon"></span></a></div>
    </div>

    <div class="g-GlHeader__navBlock">
      <ul class="g-GlHeaderNavList">
        <li><a href="http://www.kao.com/jp/corp/">会社情報</a></li>
        <li><a href="http://www.kao.com/jp/products/">製品カタログ</a></li>
        <li><a href="http://www.kao.com/jp/qa/">製品Q&amp;A</a></li>
        <li><a href="http://www.kao.com/jp/soudan/">お問い合わせ</a></li>
      </ul>
    </div>

  </div>
  </div>
</div>
<!-- / area-GlHeader -->


<!-- contentsWrapper -->
<div id="contentsWrapper" class="contentsWrapper">

  <!-- area-CntHeader -->
  <div class="g-Area" id="area-CntHeader">
    <div class="g-Area__inner">
      <div class="CntHeader">
        Contents Header Area
      </div>
    </div>
  </div>
  <!-- / area-CntHeader -->


  <!-- area-Contents -->
  <div class="g-Area" id="area-Contents">

    <div class="parsys">

      <!-- Section -->
      <div class="g-Section is-marginReset">
        <div class="g-Section__inner">
          Section (marginReset)
          <div class="parsys">
            <!-- Column -->
            <div class="g-Column is-marginReset js-uniformHeight" data-uniform-target="g-ColumnUnit">
              <div class="g-Column__inner">
                <div class="g-Column__cols g-Column__cols-lg-1 g-Column__cols-md-1 g-Column__cols-sm-1">
                  <div class="g-ColumnUnit">
                    <div class="parsys">ColumnUnit</div>
                  </div>
                </div>
              </div>
            </div>
            <!-- / Column -->
          </div>
        </div>
      </div>
      <!-- / Section -->

    </div>

  </div>
  <!-- / area-Contents -->


  <!-- area-CntFooter -->
  <div class="g-Area" id="area-CntFooter">
    <div class="g-Area__inner">
      <div class="CntFooter">
        Contents Footer Area
      </div>
    </div>
  </div>
  <!-- / area-CntFooter -->

</div>
<!-- / contentsWrapper -->


